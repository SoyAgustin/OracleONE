
public class TestCaracteres {
	public static void main(String[] args) {
		char letra = 'a';
		System.out.println(letra);
		
		char valor = 65;
		System.out.println(valor);
		
		valor = (char) (valor+1);
		System.out.println(valor);
		
		String palabra = "Alura cursos online de tecnolog√≠a";
		
		System.out.println(palabra);
		
	}
}
